{
  "name": "EasyUse",
  "description": "EasyUse",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "contextMenus",
    "downloads",
    "pageCapture"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "EasyUse"
  },
  "commands": {
      "switch-tab": {
          "suggested_key": {
              "default": "Alt+1"
          },
          "description": "switch tab"
      },
      "pinned-tab": {
          "suggested_key": {
              "default": "Alt+2"
          },
          "description": "pinned tab"
      },
      "search": {
          "suggested_key": {
              "default": "Alt+3"
          },
          "description": "search"
      },
      "save-as-MHTML": {
          "suggested_key": {
              "default": "Alt+4"
          },
          "description": "save as MHTML"
      }
  },
  "manifest_version": 2
}
